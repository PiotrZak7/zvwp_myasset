<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar" xmlns:smartTable="sap.ui.comp.smarttable">
	<Dialog id="NewDeviceService" title="{i18n>RateService}" afterClose="dialogAfterclose">
		<f:SimpleForm>
			<RatingIndicator id="riRaitingNew" maxValue="5" class="sapUiSmallMarginBottom" value="0.0" iconSize="22px" tooltip="Rate service"/>
		</f:SimpleForm>
		<beginButton>
			<Button text="{i18n>SaveVote}" press="onRateSave" type="Accept"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>cancel}" press="onCloseDialog" type="Reject"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>