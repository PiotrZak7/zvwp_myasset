<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc">
	<Dialog id="NewDeviceService" title="Rate the service" afterClose="dialogAfterclose">
		<f:SimpleForm>
			<RatingIndicator id="riRaitingNew" maxValue="5" class="sapUiSmallMarginBottom" value="0.0" iconSize="22px" tooltip="Rate service"/>
		</f:SimpleForm>
		<beginButton>
			<Button text="Save vote" press="onRateSave" type="Accept"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="onCloseDialog" type="Reject"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>