<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<VBox class="sapUiResponsiveMargin">
		<!-- Create Request for Cert -->
		<f:SimpleForm id="sfUserOverview" editable="true" layout="ResponsiveGridLayout" labelSpanXL="-1" emptySpanXL="-1" columnsXL="-1"
			labelSpanL="3" emptySpanL="4" columnsL="1" labelSpanM="3" emptySpanM="4" columnsM="1" labelSpanS="12" emptySpanS="0" adjustLabelSpan="false"
			singleContainerFullSize="true">
			<f:content>
				<Label text="{i18n>CreateOn}"/>
				<Text text="{path: 'Begda', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd MMM yyyy' } }"/>
				<Label text="{i18n>ValidTo}"/>
				<Text text="{path: 'Endda', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'dd MMM yyyy' } }"/>
				<Label text="{i18n>leasing}"/>
				<Text text="{= ${IdLeasing} === true ? 'Tak' : 'Nie' }"/>
				
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>